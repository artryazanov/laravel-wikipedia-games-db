{
  "name": "artryazanov/laravel-wikipedia-games-db",
  "description": "A Laravel package to build a video game database by scraping English Wikipedia.",
  "type": "laravel-package",
  "license": "Unlicense",
  "authors": [
    {
      "name": "Artryazanov"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "illuminate/http": "^10.0|^11.0|^12.0",
    "illuminate/database": "^10.0|^11.0|^12.0",
    "illuminate/console": "^10.0|^11.0|^12.0",
    "illuminate/queue": "^10.0|^11.0|^12.0",
    "symfony/dom-crawler": "^6.0|^7.0",
    "symfony/css-selector": "^6.0|^7.0"
  },
  "autoload": {
    "psr-4": {
      "Artryazanov\\WikipediaGamesDb\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Artryazanov\\WikipediaGamesDb\\WikipediaGamesDbServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0|^10.0",
    "phpunit/phpunit": "^10.5|^11.0",
    "laravel/pint": "^1.24"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
